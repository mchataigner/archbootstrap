! Launch urxvt as login shell !
URxvt*loginShell: true

! XFT stuff !
Xft.antialias:  true
Xft.autohint:   false
Xft.dpi:        180
Xft.hinting:    true
Xft.hintstyle:  hintslight
Xft.lcdfilter:  lcddefault
Xft.rgba:       rgb

